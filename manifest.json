{
    "manifest_version": 3,
    "name": "NewTab",
    "description": "Custom NewTab",
    "version": "1.0",
    "author": "Cl√©ment Songis",
    "action": { 
        "default_icon": "img/icon_128.png",
        "default_title": "NewTab",
    },
    "host_permissions": [
        "*://*/"
